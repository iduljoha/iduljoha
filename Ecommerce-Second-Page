    $(document).ready(function () {
      $('#user').click(function () {
        $('.popup').css("display", "block");


      });

      $('#signup').click(function () {
        $('.popup-signup').css("display", "block");
         

      });

      $('#login').click(function () {
        $('.popup-signup').css("display", "none");


